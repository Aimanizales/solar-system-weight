{
  "paths": {
    "root": {
      "bundle": "",
      "staging": "http://"
    },
    "js": {
      "app": "app/scripts/**/*.js",
      "vendor": [
        "node_modules/jquery/dist/jquery.min.js",
        "node_modules/numbro/dist/numbro.min.js"
      ]
    },
    "css": {
      "app": "app/styles/styles.scss",
      "vendor": [
        "node_modules/bootstrap/dist/css/bootstrap.min.css"
      ]
    },
    "hbs": {
      "templates": ["app/templates/**/*.hbs", "!app/templates/partials/**"],
      "partials": "app/templates/partials"
    },
    "assets": ["app/assets/**"]
  }
}
